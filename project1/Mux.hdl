/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=sel , b=b , out=SELandB );
    Not(in=sel , out=notSEL );
    And(a=a , b=notSEL , out=notSELandA );
    Or(a=notSELandA , b=SELandB , out=out );
}